<body>
  <!-- <h1>CC B2B</h1> -->
  <div class="left">
    <form action="/messages" method="POST">
      <fieldset>
        <!-- <label for="logo">Upload the Brand's Logo: <input id="logo" type="file" name="file" /></label> -->
        <label for="brand">Brand: *<input value="<%= messages[0]?.brand %>" id="brand" name="brand" type="text"
            required /></label>
        <label for="company">Company Name: *<input value="<%= messages[0]?.company %>" id="company" name="company"
            type="text" required /></label>
        <label for="year">How long have you been in business? <input value="<%= messages[0]?.year %>" id="year"
            type="number" name="year" min="0" max="2000" /></label>
        <label for="hq">Headquater: *<input value="<%= messages[0]?.hq %>" id="hq" name="hq" type="text"
            required /></label>
        <label for="location">Location(s): <input value="<%= messages[0]?.location %>" id="location" name="location"
            type="text" /></label>
        <label for="person">Contact Person: *<input value="<%= messages[0]?.person %>" id="person" name="person"
            type="text" required /></label>
        <label for="position">Position: *<input value="<%= messages[0]?.position %>" id="position" name="position"
            type="text" required /></label>
        <label for="number">Contact Number: *<input value="<%= messages[0]?.number %>" id="number" name="number"
            type="number" required /></label>
        <label for="email">Email: *<input value="<%= messages[0]?.email %>" id="email" name="email" type="email"
            required /></label>
        <label for="website">Website: *<input value="<%= messages[0]?.website %>" id="website" name="website"
            type="text" required /></label>
        <label for="why">(In 100 words or less) Let us know your purpose driven mission(s): *
          <textarea value="<%= messages[0]?.why %>" id="why" name="why" rows="3" cols="30"
            placeholder="What's your why?" required></textarea>
        </label>
        <label for="offer">Tell us about the Product or Service you offer: *
          <textarea value="<%= messages[0]?.offer %>" id="offer" name="offer" rows="3" cols="30" placeholder=""
            required></textarea>
        </label>
        <label for="looking">What are you looking for to take your company to the next level?
          <textarea value="<%= messages[0]?.looking %>" id="looking" name="looking" rows="3" cols="30"
            placeholder=""></textarea>
        </label>
        <label for="unique">What makes you unique? <input value="<%= messages[0]?.unique %>" id="different"
            name="unique" type="text" /></label>
        <label for="else">Anything else you'd like us to know about your company? <input
            value="<%= messages[0]?.else %>" id="else" name="else" type="text" /></label>
        <label for="moonshot">What's your Moonshot: <input value="<%= messages[0]?.moonshot %>" id="moonshot"
            name="moonshot" type="text" /></label>
        <!-- <label for="new-password">Create a New Password: <input id="new-password" name="new-password" type="password" pattern="[a-z0-5]{8,}" required /></label> -->
      </fieldset>
