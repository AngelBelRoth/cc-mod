  </div>

  <div class="right">
    <fieldset>
      <p>Brand:</p>
        <%= messages[0]?.brand %>
      <p>Company Name:</p>
        <%= messages[0]?.company %>
      <p>How long have you been in business?</p>
        <%= messages[0]?.year %>
      <p>Headquater:<p>
        <%= messages[0]?.hq %>
      <p>Location(s):</p>
        <%= messages[0]?.location %>
      <p>Contact Person:</p>
